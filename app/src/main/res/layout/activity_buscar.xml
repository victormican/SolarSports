<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F4EB49"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:paddingLeft="0dp"
    android:paddingBottom="0dp"
    tools:context=".Buscar">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingLeft="350dp">

        <ImageView
            android:id="@+id/imageViewExit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/logout_fill0_wght400_grad0_opsz48" />
    </LinearLayout>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="302dp"
        android:layout_height="206dp"
        android:cropToPadding="false"
        android:paddingTop="12dp"
        android:paddingBottom="12dp"
        app:srcCompat="@drawable/image_pasted_2023_20_07_at_14_24_56" />

    <TextView
        android:id="@+id/textViewSolars"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingBottom="5dp"
        android:text="SOLARSPORTS"
        android:textSize="34sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textViewTittle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingBottom="10dp"
        android:text="Buscar"
        android:textSize="20sp" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_marginStart="30dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="20dp"
        android:layout_weight="0"
        android:background="#0277BD"
        android:backgroundTintMode="add"
        android:orientation="horizontal"
        android:paddingTop="12dp"
        android:paddingBottom="12dp">

        <TextView
            android:id="@+id/textViewCancha"
            android:layout_width="186dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingLeft="30dp"
            android:paddingTop="12dp"
            android:paddingBottom="12dp"
            android:text="Buscar" />

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="164dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
