<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F4EB49"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:paddingLeft="0dp"
    android:paddingBottom="0dp"
    tools:context=".Estadisticas">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingLeft="350dp">

        <ImageView
            android:id="@+id/imageViewExit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/logout_fill0_wght400_grad0_opsz48" />
    </LinearLayout>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="302dp"
        android:layout_height="206dp"
        android:cropToPadding="false"
        android:paddingTop="12dp"
        android:paddingBottom="12dp"
        app:srcCompat="@drawable/image_pasted_2023_20_07_at_14_24_56" />

    <TextView
        android:id="@+id/textViewSolars"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingBottom="5dp"
        android:text="SOLARSPORTS"
        android:textSize="34sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textViewTittle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingBottom="10dp"
        android:text="Estadisticas"
        android:textSize="20sp" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="20dp"
        android:layout_weight="0"
        android:background="#ffa834"
        android:orientation="horizontal"
        android:paddingLeft="30dp"
        android:paddingTop="12dp"
        android:paddingBottom="12dp">

        <TextView
            android:id="@+id/textViewCategory2"
            android:layout_width="224dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="12dp"
            android:paddingBottom="12dp"
            android:text="Estadisticas"
            android:textSize="14dp"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/imageViewStadistic"
            android:layout_width="174dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:srcCompat="@drawable/query_stats_fill0_wght400_grad0_opsz48" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="20dp"
        android:layout_weight="1"
        android:background="#ffa834"
        android:orientation="horizontal"
        android:paddingTop="12dp"
        android:paddingBottom="12dp">

        <TableLayout
            android:id="@+id/tableLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="58dp"
                android:background="#283593"
                tools:layout_conversion_absoluteHeight="0dp"
                tools:layout_conversion_absoluteWidth="302dp"
                tools:layout_editor_absoluteX="60dp"
                tools:layout_editor_absoluteY="398dp">

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="54dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="MES"
                    android:textAlignment="center"
                    android:textColor="#FFFFFF"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="PROMEDIO ENERGIA"
                    android:textAlignment="center"
                    android:textColor="#FFFFFF"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="89dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="CATEGORIA"
                    android:textAlignment="center"
                    android:textColor="#FFFFFF"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="68dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="AHORRO"
                    android:textAlignment="center"
                    android:textColor="#FFFFFF"
                    android:textStyle="bold" />
            </TableRow>

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:layout_conversion_absoluteHeight="0dp"
                tools:layout_conversion_absoluteWidth="302dp"
                tools:layout_editor_absoluteX="60dp"
                tools:layout_editor_absoluteY="398dp" />

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:layout_conversion_absoluteHeight="0dp"
                tools:layout_conversion_absoluteWidth="302dp"
                tools:layout_editor_absoluteX="60dp"
                tools:layout_editor_absoluteY="398dp" />

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:layout_conversion_absoluteHeight="0dp"
                tools:layout_conversion_absoluteWidth="302dp"
                tools:layout_editor_absoluteX="60dp"
                tools:layout_editor_absoluteY="398dp" />

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:layout_conversion_absoluteHeight="0dp"
                tools:layout_conversion_absoluteWidth="302dp"
                tools:layout_editor_absoluteX="60dp"
                tools:layout_editor_absoluteY="398dp" />
        </TableLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:clipToPadding="true"
        android:gravity="bottom"
        android:orientation="horizontal"
        android:paddingTop="120dp">

        <ImageView
            android:id="@+id/imageViewHomeIcon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#0025F4"
            android:backgroundTint="#0025F4"
            app:srcCompat="@drawable/home_fill0_wght400_grad0_opsz48"
            tools:layout_conversion_absoluteHeight="44dp"
            tools:layout_conversion_absoluteWidth="52dp"
            tools:layout_editor_absoluteX="30dp"
            tools:layout_editor_absoluteY="795dp" />

        <ImageView
            android:id="@+id/imageViewSearchIcon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#0025F4"
            app:srcCompat="@drawable/search_fill0_wght400_grad0_opsz48"
            tools:layout_conversion_absoluteHeight="44dp"
            tools:layout_conversion_absoluteWidth="52dp"
            tools:layout_editor_absoluteX="82dp"
            tools:layout_editor_absoluteY="795dp" />

        <ImageView
            android:id="@+id/imageViewCategoryIcon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#0025F4"
            app:srcCompat="@drawable/bookmark_add_fill0_wght400_grad0_opsz48"
            tools:layout_conversion_absoluteHeight="44dp"
            tools:layout_conversion_absoluteWidth="52dp"
            tools:layout_editor_absoluteX="133dp"
            tools:layout_editor_absoluteY="795dp" />

        <ImageView
            android:id="@+id/imageViewRegisterIcon"
            android:layout_width="61dp"
            android:layout_height="wrap_content"
            android:background="#0025F4"
            app:srcCompat="@drawable/app_registration_fill0_wght400_grad0_opsz48"
            tools:layout_conversion_absoluteHeight="44dp"
            tools:layout_conversion_absoluteWidth="52dp"
            tools:layout_editor_absoluteX="185dp"
            tools:layout_editor_absoluteY="795dp" />

        <ImageView
            android:id="@+id/imageViewStadisticsIcon"
            android:layout_width="63dp"
            android:layout_height="wrap_content"
            android:background="#0025F4"
            app:srcCompat="@drawable/query_stats_fill0_wght400_grad0_opsz48"
            tools:layout_conversion_absoluteHeight="44dp"
            tools:layout_conversion_absoluteWidth="52dp"
            tools:layout_editor_absoluteX="237dp"
            tools:layout_editor_absoluteY="795dp" />

        <ImageView
            android:id="@+id/imageViewBenefitsIcon"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:background="#0025F4"
            app:srcCompat="@drawable/loyalty_fill0_wght400_grad0_opsz48"
            tools:layout_conversion_absoluteHeight="44dp"
            tools:layout_conversion_absoluteWidth="52dp"
            tools:layout_editor_absoluteX="289dp"
            tools:layout_editor_absoluteY="795dp" />

        <ImageView
            android:id="@+id/imageViewUserIcon"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:background="#0025F4"
            app:srcCompat="@drawable/account_circle_fill0_wght400_grad0_opsz48"
            tools:layout_conversion_absoluteHeight="44dp"
            tools:layout_conversion_absoluteWidth="52dp"
            tools:layout_editor_absoluteX="341dp"
            tools:layout_editor_absoluteY="795dp" />
    </LinearLayout>

</androidx.appcompat.widget.LinearLayoutCompat>