<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F4EB49"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:paddingLeft="0dp"
    android:paddingBottom="0dp"
    tools:context=".RegistroGim">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingLeft="350dp">

        <ImageView
            android:id="@+id/imageViewExit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/logout_fill0_wght400_grad0_opsz48" />
    </LinearLayout>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="302dp"
        android:layout_height="206dp"
        android:cropToPadding="false"
        android:paddingTop="12dp"
        android:paddingBottom="12dp"
        app:srcCompat="@drawable/image_pasted_2023_20_07_at_14_24_56" />

    <TextView
        android:id="@+id/textViewSolars"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingBottom="5dp"
        android:text="SOLARSPORTS"
        android:textSize="34sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textViewTittle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingBottom="10dp"
        android:text="Registro"
        android:textSize="20sp" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="20dp"
        android:layout_weight="0"
        android:background="#91e4fb"
        android:orientation="horizontal"
        android:paddingStart="30dp"
        android:paddingTop="0dp"
        android:paddingEnd="30dp"
        android:paddingBottom="0dp">

        <TextView
            android:id="@+id/textViewRRGim"
            android:layout_width="155dp"
            android:layout_height="23dp"
            android:layout_weight="1"
            android:text="Gimnasios" />

        <ImageView
            android:id="@+id/imageViewRRGimnasios"
            android:layout_width="134dp"
            android:layout_height="78dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/_b4fbe906c63b6cb3e95a30f1bc14dd6" />

    </LinearLayout>

    <TextView
        android:id="@+id/textViewTittle2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingBottom="10dp"
        android:text="Registro Consumo"
        android:textSize="20sp" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:layout_weight="0"
        android:orientation="horizontal"
        android:paddingTop="0dp"
        android:paddingBottom="0dp">

        <TextView
            android:id="@+id/textViewNombreGim"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0"
            android:text="Nombre Gimnasio: " />

        <EditText
            android:id="@+id/editTextNombreGim"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:layout_weight="0"
            android:ems="25"
            android:hint="Nombre Gimnasio"
            android:inputType="text" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_weight="0"
        android:orientation="horizontal"
        android:paddingTop="0dp"
        android:paddingBottom="0dp">

        <TextView
            android:id="@+id/textViewConsumoKW"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0"
            android:text="Consumo en KW :" />

        <EditText
            android:id="@+id/editTextConsumoKW"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:layout_weight="0"
            android:ems="25"
            android:hint="Consumo KW"
            android:inputType="text" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:layout_weight="0"
        android:orientation="horizontal"
        android:paddingTop="0dp"
        android:paddingBottom="10dp">

        <TextView
            android:id="@+id/textViewValorKW"
            android:layout_width="108dp"
            android:layout_height="wrap_content"
            android:layout_weight="0"
            android:text="Valor KW :" />

        <EditText
            android:id="@+id/editTextValorKW"
            android:layout_width="263dp"
            android:layout_height="48dp"
            android:layout_weight="0"
            android:ems="25"
            android:hint="Valor KW"
            android:inputType="text" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:layout_weight="0"
        android:orientation="horizontal"
        android:paddingTop="0dp"
        android:paddingBottom="0dp">

        <TextView
            android:id="@+id/textViewMes"
            android:layout_width="104dp"
            android:layout_height="wrap_content"
            android:layout_weight="0"
            android:text="Mes Registro : " />

        <EditText
            android:id="@+id/editTextMes"
            android:layout_width="249dp"
            android:layout_height="48dp"
            android:layout_weight="0"
            android:ems="25"
            android:hint="Mes Resgistro"
            android:inputType="text" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:orientation="horizontal"
        android:paddingTop="0dp"
        android:paddingBottom="5dp">

        <ImageView
            android:id="@+id/imageViewBack"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0"
            app:srcCompat="@drawable/arrow_back_ios_fill0_wght400_grad0_opsz48" />

        <Button
            android:id="@+id/btnRegistrar"
            android:layout_width="157dp"
            android:layout_height="wrap_content"
            android:layout_weight="0"
            android:backgroundTint="#f7fade"
            android:text="Registrar"
            android:textColor="#000000" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:clipToPadding="true"
        android:gravity="bottom"
        android:orientation="horizontal"
        android:paddingTop="15dp">

        <ImageView
            android:id="@+id/imageViewHomeIcon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#0025F4"
            android:backgroundTint="#0025F4"
            app:srcCompat="@drawable/home_fill0_wght400_grad0_opsz48"
            tools:layout_conversion_absoluteHeight="44dp"
            tools:layout_conversion_absoluteWidth="52dp"
            tools:layout_editor_absoluteX="30dp"
            tools:layout_editor_absoluteY="795dp" />

        <ImageView
            android:id="@+id/imageViewSearchIcon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#0025F4"
            app:srcCompat="@drawable/search_fill0_wght400_grad0_opsz48"
            tools:layout_conversion_absoluteHeight="44dp"
            tools:layout_conversion_absoluteWidth="52dp"
            tools:layout_editor_absoluteX="82dp"
            tools:layout_editor_absoluteY="795dp" />

        <ImageView
            android:id="@+id/imageViewCategoryIcon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#0025F4"
            app:srcCompat="@drawable/bookmark_add_fill0_wght400_grad0_opsz48"
            tools:layout_conversion_absoluteHeight="44dp"
            tools:layout_conversion_absoluteWidth="52dp"
            tools:layout_editor_absoluteX="133dp"
            tools:layout_editor_absoluteY="795dp" />

        <ImageView
            android:id="@+id/imageViewRegisterIcon"
            android:layout_width="61dp"
            android:layout_height="wrap_content"
            android:background="#0025F4"
            app:srcCompat="@drawable/app_registration_fill0_wght400_grad0_opsz48"
            tools:layout_conversion_absoluteHeight="44dp"
            tools:layout_conversion_absoluteWidth="52dp"
            tools:layout_editor_absoluteX="185dp"
            tools:layout_editor_absoluteY="795dp" />

        <ImageView
            android:id="@+id/imageViewStadisticsIcon"
            android:layout_width="63dp"
            android:layout_height="wrap_content"
            android:background="#0025F4"
            app:srcCompat="@drawable/query_stats_fill0_wght400_grad0_opsz48"
            tools:layout_conversion_absoluteHeight="44dp"
            tools:layout_conversion_absoluteWidth="52dp"
            tools:layout_editor_absoluteX="237dp"
            tools:layout_editor_absoluteY="795dp" />

        <ImageView
            android:id="@+id/imageViewBenefitsIcon"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:background="#0025F4"
            app:srcCompat="@drawable/loyalty_fill0_wght400_grad0_opsz48"
            tools:layout_conversion_absoluteHeight="44dp"
            tools:layout_conversion_absoluteWidth="52dp"
            tools:layout_editor_absoluteX="289dp"
            tools:layout_editor_absoluteY="795dp" />

        <ImageView
            android:id="@+id/imageViewUserIcon"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:background="#0025F4"
            app:srcCompat="@drawable/account_circle_fill0_wght400_grad0_opsz48"
            tools:layout_conversion_absoluteHeight="44dp"
            tools:layout_conversion_absoluteWidth="52dp"
            tools:layout_editor_absoluteX="341dp"
            tools:layout_editor_absoluteY="795dp" />
    </LinearLayout>

</androidx.appcompat.widget.LinearLayoutCompat>